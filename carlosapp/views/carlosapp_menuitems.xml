<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="registry_list_action" model ="ir.actions.act_window">
        <field name="name">Course List</field>
        <field name="res_model">carlosapp.registry</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create the first registry
            </p>
        </field>
    </record>
    <menuitem name="Moto Kaiiwi" 
        id="menu_carlosapp"
        sequence="1" 
        web_icon="carlosapp,static/description/icon.png">
        <menuitem id="menu_carlosapp_sub" name="menu_carlosapp" sequence="1">
            <menuitem id="menu_carlosapp_registry" name="Registry" sequence="2" action="registry_list_action"/>
        </menuitem>
    </menuitem> 
</odoo>